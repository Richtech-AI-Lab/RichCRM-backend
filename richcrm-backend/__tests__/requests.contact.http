content-type: application/json

{
    "contactType": 0,
    "firstName": "Lawson",
    "lastName": "Wu",
    "company": "RichTech",
    "position": "CTO",
    "email": "test@gmail.com",
    "cellNumber": "0912345678",
    "mailingAddress": "Anchorage AK 99501-2016",
